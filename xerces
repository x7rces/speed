local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local Lighting = game:GetService("Lighting")

local player = Players.LocalPlayer
while not player do
    task.wait()
    player = Players.LocalPlayer
end
local playerGui = player:WaitForChild("PlayerGui")

local logBuffer = {}
local maxBufferSize = 100
local flushInterval = 0.01
local oldPrint = print
local oldWarn = warn
local oldError = error

local function instantLog(message, logType)
    logType = logType or "INFO"
   
    if rconsoleprint then
        local timestamp = os.date("%H:%M:%S")
        local prefix = ""
       
        if logType == "WARN" then
            prefix = "âš ï¸ "
        elseif logType == "ERROR" then
            prefix = "âŒ "
        else
            prefix = "âœ“ "
        end
       
        rconsoleprint(string.format("[%s] %s%s\n", timestamp, prefix, tostring(message)))
    end
   
    table.insert(logBuffer, {
        time = tick(),
        message = message,
        type = logType
    })
   
    if #logBuffer > maxBufferSize then
        table.remove(logBuffer, 1)
    end
end

print = function(...)
    local args = {...}
    local message = table.concat({...}, " ")
    oldPrint(...)
    instantLog(message, "INFO")
end

warn = function(...)
    local message = table.concat({...}, " ")
    oldWarn(...)
    instantLog(message, "WARN")
end

task.spawn(function()
    while task.wait(flushInterval) do
        if rconsoleclear and #logBuffer > 50 then
        end
    end
end)

local function optimizeLogs()
    if setfflag then
        pcall(function()
            setfflag("AbuseReportScreenshotPercentage", "0")
            setfflag("DFFlagDebugAnalytics", "false")
            setfflag("DFFlagDebugVisualizationsEnabled", "false")
            setfflag("FFlagDebugForceFutureIsBrightPhase2", "false")
            setfflag("FFlagDebugForceFutureIsBrightPhase3", "false")
            setfflag("DFFlagDebugEnableLogToCloudForAllPlayers", "false")
        end)
    end
   
    if rconsoleclear then
        rconsoleclear()
    end
   
    if rconsoleinfo then
        rconsoleinfo("âš¡ XERCES SPEED - PREMIUM EDITION")
        rconsoleinfo("âœ“ Console forwarding: INSTANT MODE")
    end
   
    if setconsoletitle then
        setconsoletitle("XERCES SPEED - PREMIUM")
    end
   
    instantLog("Executor optimization complete", "INFO")
end

optimizeLogs()

local fpsBoostEnabled = false
local loadStartTime = tick()
local autoFPSBoost = false
local nightModeEnabled = true

local function saveSettings()
    pcall(function()
        writefile("xerces_speed_settings.json", game:GetService("HttpService"):JSONEncode({
            autoFPSBoost = autoFPSBoost,
            nightModeEnabled = nightModeEnabled
        }))
    end)
end

local function loadSettings()
    pcall(function()
        if isfile("xerces_speed_settings.json") then
            local data = game:GetService("HttpService"):JSONDecode(readfile("xerces_speed_settings.json"))
            autoFPSBoost = data.autoFPSBoost or false
            nightModeEnabled = data.nightModeEnabled or false
            instantLog("Settings loaded: Auto FPS Boost = " .. tostring(autoFPSBoost), "INFO")
        end
    end)
end

loadSettings()

local function applyNightMode()
    pcall(function()
        local lighting = game:GetService("Lighting")
        if lighting then
            lighting.ClockTime = 0
            lighting.Brightness = 3
            lighting.Ambient = Color3.fromRGB(255, 255, 255)
            lighting.OutdoorAmbient = Color3.fromRGB(255, 255, 255)
            lighting.ColorShift_Top = Color3.fromRGB(0, 0, 0)
            lighting.GlobalShadows = false
        end
    end)
   
    task.spawn(function()
        while task.wait(1) do
            pcall(function()
                local lighting = game:GetService("Lighting")
                if lighting then
                    if lighting.ClockTime ~= 0 then
                        lighting.ClockTime = 0
                    end
                    if lighting.Brightness ~= 3 then
                        lighting.Brightness = 3
                    end
                    lighting.Ambient = Color3.fromRGB(255, 255, 255)
                    lighting.OutdoorAmbient = Color3.fromRGB(255, 255, 255)
                    lighting.GlobalShadows = false
                end
            end)
        end
    end)
end

local function toggleFPSBoost()
    if getgenv().POTATO_OPT_ACTIVE then
        getgenv().POTATO_OPT_ACTIVE = false
        fpsBoostEnabled = false
        instantLog("FPS BOOST DISABLED", "INFO")
       
        pcall(function()
            local lighting = game:GetService("Lighting")
            if lighting then
                lighting.GlobalShadows = true
                lighting.Brightness = 2
                if lighting.Technology then
                    lighting.Technology = Enum.Technology.ShadowMap
                end
            end
        end)
       
        return
    end
   
    getgenv().POTATO_OPT_ACTIVE = true
    fpsBoostEnabled = true
    instantLog("FPS BOOST ENABLED - Applying potato optimizations", "INFO")
   
    local threads = {}
    local connections = {}
   
    local function addThread(f)
        local t = task.spawn(f)
        threads[#threads+1] = t
    end
   
    local function addConn(c)
        connections[#connections+1] = c
    end
   
    local FFLAGS = {
        DFIntTaskSchedulerTargetFps = 999,
        FFlagDisablePostFx = true,
        FIntRenderShadowIntensity = 0,
        DFIntTextureQualityOverride = 1,
        FFlagEnableWaterReflections = false,
    }
   
    pcall(function()
        for k,v in pairs(FFLAGS) do
            setfflag(k, tostring(v))
        end
    end)
   
    pcall(function()
        local r = settings().Rendering
        r.QualityLevel = Enum.QualityLevel.Level01
        r.MeshPartDetailLevel = Enum.MeshPartDetailLevel.Level01
    end)
   
    pcall(function()
        local lighting = game:GetService("Lighting")
        if lighting then
            lighting.GlobalShadows = false
            lighting.Brightness = 3
            lighting.FogEnd = 9e9
            if lighting.Technology then
                lighting.Technology = Enum.Technology.Legacy
            end
            lighting.EnvironmentDiffuseScale = 0
            lighting.EnvironmentSpecularScale = 0
            for _,v in ipairs(lighting:GetChildren()) do
                if v:IsA("PostEffect") or v:IsA("Atmosphere") or v:IsA("BloomEffect")
                    or v:IsA("BlurEffect") or v:IsA("ColorCorrectionEffect")
                    or v:IsA("DepthOfFieldEffect") or v:IsA("SunRaysEffect") then
                    v:Destroy()
                end
            end
        end
    end)
   
    pcall(function()
        local p = settings().Physics
        p.AllowSleep = true
        p.PhysicsEnvironmentalThrottle = Enum.EnviromentalPhysicsThrottle.Skip
        p.ThrottleAdjustTime = 0
    end)
   
    pcall(function()
        local t = workspace.Terrain
        t.Decoration = false
        t.WaterWaveSize = 0
        t.WaterWaveSpeed = 0
        t.WaterReflectance = 0
        t.WaterTransparency = 1
    end)
   
    local function optimizeObj(o)
        if o:IsA("BasePart") then
            o.CastShadow = false
            o.Material = Enum.Material.Plastic
            o.Reflectance = 0
        elseif o:IsA("Decal") or o:IsA("Texture") then
            if not (o.Name == "face" and o.Parent and o.Parent.Name == "Head") then
                o.Transparency = 1
            end
        elseif o:IsA("SpecialMesh") then
            o.TextureId = ""
        elseif o:IsA("ParticleEmitter")
        or o:IsA("Trail")
        or o:IsA("Beam")
        or o:IsA("PointLight")
        or o:IsA("SpotLight")
        or o:IsA("SurfaceLight")
        or o:IsA("Fire")
        or o:IsA("Smoke")
        or o:IsA("Sparkles")
        or o:IsA("Explosion") then
            o:Destroy()
        end
    end
   
    for _,o in ipairs(workspace:GetDescendants()) do
        pcall(optimizeObj, o)
    end
   
    addConn(workspace.DescendantAdded:Connect(function(o)
        if getgenv().POTATO_OPT_ACTIVE then
            pcall(optimizeObj, o)
        end
    end))
   
    local function optimizeChar(char)
        task.wait(0.5)
        for _,o in ipairs(char:GetDescendants()) do
            pcall(optimizeObj, o)
        end
    end
   
    for _,plr in ipairs(Players:GetPlayers()) do
        if plr.Character then optimizeChar(plr.Character) end
        addConn(plr.CharacterAdded:Connect(optimizeChar))
    end
   
    addConn(Players.PlayerAdded:Connect(function(plr)
        addConn(plr.CharacterAdded:Connect(optimizeChar))
    end))
   
    addThread(function()
        while getgenv().POTATO_OPT_ACTIVE do
            task.wait(30)
            collectgarbage("collect")
        end
    end)
   
    pcall(function()
        setfpscap(999)
    end)
   
    pcall(function()
        workspace.CurrentCamera.FieldOfView = 70
    end)
   
    instantLog("FPS BOOST ACTIVE - Potato mode enabled", "INFO")
end

UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
   
    if input.KeyCode == Enum.KeyCode.L then
        local ctrlHeld = UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) or UserInputService:IsKeyDown(Enum.KeyCode.RightControl)
       
        if ctrlHeld then
            autoFPSBoost = not autoFPSBoost
            saveSettings()
           
            if autoFPSBoost then
                instantLog("AUTO FPS BOOST ENABLED", "INFO")
                if not fpsBoostEnabled then
                    toggleFPSBoost()
                end
            else
                instantLog("AUTO FPS BOOST DISABLED", "INFO")
            end
        else
            toggleFPSBoost()
        end
    end
end)

local function createPremiumLoadingScreen()
    local playerGui = player:WaitForChild("PlayerGui")
   
    if playerGui:FindFirstChild("XercesSpeed") then
        playerGui.XercesSpeed:Destroy()
    end
   
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "XercesSpeed"
    screenGui.ResetOnSpawn = false
    screenGui.DisplayOrder = 999999
    screenGui.IgnoreGuiInset = true
    screenGui.Parent = playerGui
   
    local overlay = Instance.new("Frame")
    overlay.Size = UDim2.new(1, 0, 1, 0)
    overlay.BackgroundColor3 = Color3.fromRGB(10, 10, 15)
    overlay.BackgroundTransparency = 0.3
    overlay.BorderSizePixel = 0
    overlay.Parent = screenGui
   
    local gradient = Instance.new("UIGradient")
    gradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(15, 15, 25)),
        ColorSequenceKeypoint.new(0.5, Color3.fromRGB(25, 15, 35)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(15, 15, 25))
    }
    gradient.Rotation = 45
    gradient.Parent = overlay
   
    task.spawn(function()
        while overlay.Parent do
            for i = 0, 360, 4 do
                if not overlay.Parent then break end
                gradient.Rotation = i
                task.wait(0.02)
            end
        end
    end)
   
    local container = Instance.new("Frame")
    container.Size = UDim2.new(1, 0, 1, 0)
    container.BackgroundTransparency = 1
    container.Parent = screenGui
   
    for i = 1, 12 do
        local particle = Instance.new("Frame")
        particle.Size = UDim2.new(0, math.random(2, 4), 0, math.random(2, 4))
        particle.Position = UDim2.new(math.random(), 0, math.random(), 0)
        particle.BackgroundColor3 = Color3.fromRGB(150, 100, 255)
        particle.BackgroundTransparency = math.random(30, 70) / 100
        particle.BorderSizePixel = 0
        particle.Parent = container
       
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(1, 0)
        corner.Parent = particle
       
        task.spawn(function()
            while container.Parent do
                local randomX = math.random()
                local randomY = math.random()
                local randomTime = math.random(20, 40) / 10
               
                TweenService:Create(particle,
                    TweenInfo.new(randomTime, Enum.EasingStyle.Linear),
                    {Position = UDim2.new(randomX, 0, randomY, 0)}
                ):Play()
               
                task.wait(randomTime)
            end
        end)
    end
   
    for i = 1, 5 do
        local scanline = Instance.new("Frame")
        scanline.Size = UDim2.new(1, 0, 0, 3)
        scanline.Position = UDim2.new(0, 0, 0, -50 - (i * 60))
        scanline.BackgroundColor3 = Color3.fromRGB(120, 80, 255)
        scanline.BackgroundTransparency = 0.6
        scanline.BorderSizePixel = 0
        scanline.Parent = container
       
        local glow = Instance.new("UIGradient")
        glow.Transparency = NumberSequence.new{
            NumberSequenceKeypoint.new(0, 1),
            NumberSequenceKeypoint.new(0.5, 0),
            NumberSequenceKeypoint.new(1, 1)
        }
        glow.Parent = scanline
       
        task.spawn(function()
            while container.Parent do
                scanline.Position = UDim2.new(0, 0, 0, -50 - (i * 60))
                TweenService:Create(scanline,
                    TweenInfo.new(1.5, Enum.EasingStyle.Linear),
                    {Position = UDim2.new(0, 0, 1.2, 0)}
                ):Play()
                task.wait(1.5)
            end
        end)
    end
   
    local logoContainer = Instance.new("Frame")
    logoContainer.Size = UDim2.new(0, 900, 0, 200)
    logoContainer.Position = UDim2.new(0.5, -450, 0.5, -100)
    logoContainer.BackgroundTransparency = 1
    logoContainer.Parent = container
   
    local xercesText = Instance.new("TextLabel")
    xercesText.Size = UDim2.new(0, 450, 0, 200)
    xercesText.Position = UDim2.new(0, -500, 0, 0)
    xercesText.BackgroundTransparency = 1
    xercesText.Text = "XERCES"
    xercesText.Font = Enum.Font.GothamBlack
    xercesText.TextSize = 100
    xercesText.TextColor3 = Color3.fromRGB(200, 150, 255)
    xercesText.TextStrokeTransparency = 0
    xercesText.TextStrokeColor3 = Color3.fromRGB(100, 50, 200)
    xercesText.TextTransparency = 0
    xercesText.TextXAlignment = Enum.TextXAlignment.Right
    xercesText.Parent = logoContainer
   
    local speedText = Instance.new("TextLabel")
    speedText.Size = UDim2.new(0, 400, 0, 200)
    speedText.Position = UDim2.new(1, 100, 0, 0)
    speedText.BackgroundTransparency = 1
    speedText.Text = "SPEED"
    speedText.Font = Enum.Font.GothamBlack
    speedText.TextSize = 100
    speedText.TextColor3 = Color3.fromRGB(255, 255, 255)
    speedText.TextStrokeTransparency = 0
    speedText.TextStrokeColor3 = Color3.fromRGB(150, 100, 255)
    speedText.TextTransparency = 0
    speedText.TextXAlignment = Enum.TextXAlignment.Left
    speedText.Parent = logoContainer
   
    local progressBarBg = Instance.new("Frame")
    progressBarBg.Size = UDim2.new(0, 600, 0, 6)
    progressBarBg.Position = UDim2.new(0.5, -300, 0.68, 0)
    progressBarBg.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
    progressBarBg.BorderSizePixel = 0
    progressBarBg.Parent = container
   
    local progressBarCorner = Instance.new("UICorner")
    progressBarCorner.CornerRadius = UDim.new(0, 3)
    progressBarCorner.Parent = progressBarBg
   
    local progressBar = Instance.new("Frame")
    progressBar.Size = UDim2.new(0, 0, 1, 0)
    progressBar.BackgroundColor3 = Color3.fromRGB(150, 100, 255)
    progressBar.BorderSizePixel = 0
    progressBar.Parent = progressBarBg
   
    local progressCorner = Instance.new("UICorner")
    progressCorner.CornerRadius = UDim.new(0, 3)
    progressCorner.Parent = progressBar
   
    local progressGradient = Instance.new("UIGradient")
    progressGradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(120, 80, 220)),
        ColorSequenceKeypoint.new(0.5, Color3.fromRGB(180, 120, 255)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(120, 80, 220))
    }
    progressGradient.Parent = progressBar
   
    local infoText = Instance.new("TextLabel")
    infoText.Size = UDim2.new(0, 600, 0, 40)
    infoText.Position = UDim2.new(0.5, -300, 0.72, 0)
    infoText.BackgroundTransparency = 1
    infoText.Text = "XERCES SPEED â€¢ Initializing Optimizations..."
    infoText.Font = Enum.Font.GothamBold
    infoText.TextSize = 16
    infoText.TextColor3 = Color3.fromRGB(180, 180, 200)
    infoText.TextTransparency = 1
    infoText.Parent = container
   
    local function createGlowBracket(xPos, yPos, rotation)
        local bracketContainer = Instance.new("Frame")
        bracketContainer.Size = UDim2.new(0, 60, 0, 60)
        bracketContainer.Position = UDim2.new(xPos, 0, yPos, 0)
        bracketContainer.BackgroundTransparency = 1
        bracketContainer.Rotation = rotation
        bracketContainer.Parent = container
       
        local hLine = Instance.new("Frame")
        hLine.Size = UDim2.new(0, 50, 0, 3)
        hLine.Position = UDim2.new(0, 0, 0, 0)
        hLine.BackgroundColor3 = Color3.fromRGB(150, 100, 255)
        hLine.BorderSizePixel = 0
        hLine.Parent = bracketContainer
       
        local vLine = Instance.new("Frame")
        vLine.Size = UDim2.new(0, 3, 0, 50)
        vLine.Position = UDim2.new(0, 0, 0, 0)
        vLine.BackgroundColor3 = Color3.fromRGB(150, 100, 255)
        vLine.BorderSizePixel = 0
        vLine.Parent = bracketContainer
       
        return bracketContainer
    end
   
    local brackets = {
        createGlowBracket(0.15, 0.25, 0),
        createGlowBracket(0.85, 0.25, 90),
        createGlowBracket(0.85, 0.75, 180),
        createGlowBracket(0.15, 0.75, 270),
    }
   
    local sound = Instance.new("Sound")
    sound.SoundId = "rbxassetid://3320590485"
    sound.Volume = 2.6
    sound.Parent = workspace
   
    task.spawn(function()
        task.wait(0.1)
        pcall(function() sound:Play() end)
    end)
   
    task.spawn(function()
        task.wait(2)
        sound:Destroy()
    end)
   
    task.spawn(function()
        task.wait(0.2)
       
