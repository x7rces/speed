        task.wait(0.4)
       
        TweenService:Create(xercesText, TweenInfo.new(0.35, Enum.EasingStyle.Exponential, Enum.EasingDirection.Out),
            {Position = UDim2.new(0, 0, 0, 0)}):Play()
       
        task.wait(0.1)
       
        TweenService:Create(speedText, TweenInfo.new(0.35, Enum.EasingStyle.Exponential, Enum.EasingDirection.Out),
            {Position = UDim2.new(0, 470, 0, 0)}):Play()
       
        task.wait(0.4)
       
        infoText.Text = "GAME IS LOADED!"
       
        task.wait(0.2)
       
        TweenService:Create(xercesText, TweenInfo.new(0.25, Enum.EasingStyle.Back, Enum.EasingDirection.In),
            {Position = UDim2.new(0, -500, 0, 0), TextTransparency = 1}):Play()
       
        TweenService:Create(speedText, TweenInfo.new(0.25, Enum.EasingStyle.Back, Enum.EasingDirection.In),
            {Position = UDim2.new(1, 100, 0, 0), TextTransparency = 1}):Play()
       
        TweenService:Create(overlay, TweenInfo.new(0.3), {BackgroundTransparency = 1}):Play()
        TweenService:Create(progressBarBg, TweenInfo.new(0.2), {BackgroundTransparency = 1}):Play()
        TweenService:Create(progressBar, TweenInfo.new(0.2), {BackgroundTransparency = 1}):Play()
        TweenService:Create(infoText, TweenInfo.new(0.2), {TextTransparency = 1}):Play()
       
        for _, bracket in pairs(brackets) do
            for _, child in pairs(bracket:GetDescendants()) do
                if child:IsA("Frame") then
                    TweenService:Create(child, TweenInfo.new(0.2), {BackgroundTransparency = 1}):Play()
                end
            end
        end
       
        task.wait(0.4)
        screenGui:Destroy()
    end)
   
    return screenGui
end

local function createPremiumHUD()
    local playerGui = player:WaitForChild("PlayerGui")
   
    local loadTime = string.format("%.2f", tick() - loadStartTime)
   
    local hudGui = Instance.new("ScreenGui")
    hudGui.Name = "XercesHUD"
    hudGui.ResetOnSpawn = false
    hudGui.DisplayOrder = 999998
    hudGui.IgnoreGuiInset = true
    hudGui.Parent = playerGui
   
    local mainContainer = Instance.new("Frame")
    mainContainer.Size = UDim2.new(0, 260, 0, 80)
    mainContainer.Position = UDim2.new(0.5, -130, 0, 15)
    mainContainer.BackgroundTransparency = 1
    mainContainer.Parent = hudGui
   
    local topBar = Instance.new("Frame")
    topBar.Size = UDim2.new(1, 0, 0, 40)
    topBar.Position = UDim2.new(0, 0, 0, 0)
    topBar.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
    topBar.BackgroundTransparency = 0.4
    topBar.BorderSizePixel = 0
    topBar.Parent = mainContainer
   
    local topCorner = Instance.new("UICorner")
    topCorner.CornerRadius = UDim.new(0, 8)
    topCorner.Parent = topBar
   
    local topStroke = Instance.new("UIStroke")
    topStroke.Color = Color3.fromRGB(150, 100, 255)
    topStroke.Thickness = 2
    topStroke.Transparency = 0.3
    topStroke.Parent = topBar
   
    local topGradient = Instance.new("UIGradient")
    topGradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(30, 30, 45)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(20, 20, 30))
    }
    topGradient.Rotation = 90
    topGradient.Parent = topBar
   
    local logoText = Instance.new("TextLabel")
    logoText.Size = UDim2.new(1, -20, 1, 0)
    logoText.Position = UDim2.new(0, 10, 0, 0)
    logoText.BackgroundTransparency = 1
    logoText.Text = string.format("âš¡ XERCES SPEED â€¢ %ss", loadTime)
    logoText.Font = Enum.Font.GothamBlack
    logoText.TextSize = 14
    logoText.TextColor3 = Color3.fromRGB(200, 150, 255)
    logoText.TextXAlignment = Enum.TextXAlignment.Center
    logoText.TextStrokeTransparency = 0.7
    logoText.Parent = topBar
   
    task.spawn(function()
        while task.wait(2) do
            TweenService:Create(topStroke, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut),
                {Transparency = 0}):Play()
            task.wait(0.5)
            TweenService:Create(topStroke, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut),
                {Transparency = 0.3}):Play()
        end
    end)
   
    local statsPanel = Instance.new("Frame")
    statsPanel.Size = UDim2.new(1, 0, 0, 35)
    statsPanel.Position = UDim2.new(0, 0, 0, 45)
    statsPanel.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
    statsPanel.BackgroundTransparency = 0.4
    statsPanel.BorderSizePixel = 0
    statsPanel.Parent = mainContainer
   
    local statsCorner = Instance.new("UICorner")
    statsCorner.CornerRadius = UDim.new(0, 8)
    statsCorner.Parent = statsPanel
   
    local statsStroke = Instance.new("UIStroke")
    statsStroke.Color = Color3.fromRGB(150, 100, 255)
    statsStroke.Thickness = 2
    statsStroke.Transparency = 0.5
    statsStroke.Parent = statsPanel
   
    local statsGradient = Instance.new("UIGradient")
    statsGradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(25, 25, 40)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(15, 15, 25))
    }
    statsGradient.Rotation = 135
    statsGradient.Parent = statsPanel
   
    local statsLabel = Instance.new("TextLabel")
    statsLabel.Size = UDim2.new(1, -20, 1, 0)
    statsLabel.Position = UDim2.new(0, 10, 0, 0)
    statsLabel.BackgroundTransparency = 1
    statsLabel.Text = "FPS: -- | PING: -- | BOOST: ðŸ”´ OFF"
    statsLabel.Font = Enum.Font.GothamBold
    statsLabel.TextSize = 12
    statsLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
    statsLabel.TextXAlignment = Enum.TextXAlignment.Center
    statsLabel.Parent = statsPanel
   
    task.spawn(function()
        local frameCount = 0
        local lastFPSUpdate = tick()
        local currentFPS = 60
       
        RunService.Heartbeat:Connect(function()
            frameCount = frameCount + 1
            local now = tick()
            if now - lastFPSUpdate >= 1 then
                currentFPS = frameCount
                frameCount = 0
                lastFPSUpdate = now
            end
        end)
       
        while task.wait(0.1) do
            local ping = 0
            pcall(function()
                ping = game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValue()
            end)
           
            local fpsColorCode = "<font color=\"rgb(100,255,150)\">"
            if currentFPS < 30 then
                fpsColorCode = "<font color=\"rgb(255,100,100)\">"
            elseif currentFPS < 60 then
                fpsColorCode = "<font color=\"rgb(255,200,100)\">"
            end
           
            local pingColorCode = "<font color=\"rgb(100,255,150)\">"
            if ping > 150 then
                pingColorCode = "<font color=\"rgb(255,100,100)\">"
            elseif ping > 80 then
                pingColorCode = "<font color=\"rgb(255,200,100)\">"
            end
           
            local boostText = ""
            local boostColorCode = ""
            if fpsBoostEnabled then
                boostText = autoFPSBoost and "ðŸŸ¢ ON (AUTO)" or "ðŸŸ¢ ON"
                boostColorCode = "<font color=\"rgb(100,255,150)\">"
            else
                boostText = autoFPSBoost and "ðŸŸ¡ AUTO" or "ðŸ”´ OFF"
                boostColorCode = autoFPSBoost and "<font color=\"rgb(255,200,100)\">" or "<font color=\"rgb(255,100,100)\">"
            end
           
            statsLabel.RichText = true
            statsLabel.Text = string.format(
                "%sFPS: %d</font> | %sPING: %.0f</font> | %sBOOST: %s</font>",
                fpsColorCode, currentFPS,
                pingColorCode, ping,
                boostColorCode, boostText
            )
        end
    end)
   
    local hintPanel = Instance.new("Frame")
    hintPanel.Size = UDim2.new(0, 240, 0, 55)
    hintPanel.Position = UDim2.new(0, 20, 1, -75)
    hintPanel.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
    hintPanel.BackgroundTransparency = 0.4
    hintPanel.BorderSizePixel = 0
    hintPanel.Parent = hudGui
   
    local hintCorner = Instance.new("UICorner")
    hintCorner.CornerRadius = UDim.new(0, 8)
    hintCorner.Parent = hintPanel
   
    local hintStroke = Instance.new("UIStroke")
    hintStroke.Color = Color3.fromRGB(255, 200, 100)
    hintStroke.Thickness = 2
    hintStroke.Transparency = 0.5
    hintStroke.Parent = hintPanel
   
    local hintText = Instance.new("TextLabel")
    hintText.Size = UDim2.new(1, -20, 1, 0)
    hintText.Position = UDim2.new(0, 10, 0, 0)
    hintText.BackgroundTransparency = 1
    hintText.Text = "âš¡ [L] - Toggle FPS Boost\nðŸ”„ [CTRL+L] - AUTO FPS BOOST"
    hintText.Font = Enum.Font.GothamBold
    hintText.TextSize = 12
    hintText.TextColor3 = Color3.fromRGB(255, 220, 150)
    hintText.TextXAlignment = Enum.TextXAlignment.Left
    hintText.TextYAlignment = Enum.TextYAlignment.Center
    hintText.Parent = hintPanel
   
    task.spawn(function()
        task.wait(20)
        TweenService:Create(hintPanel, TweenInfo.new(1.5), {BackgroundTransparency = 1}):Play()
        TweenService:Create(hintStroke, TweenInfo.new(1.5), {Transparency = 1}):Play()
        TweenService:Create(hintText, TweenInfo.new(1.5), {TextTransparency = 1}):Play()
    end)
end

task.spawn(function()
    while task.wait(0.05) do
        for _, gui in pairs(player.PlayerGui:GetChildren()) do
            if gui:IsA("ScreenGui") and gui.Name ~= "XercesSpeed" and gui.Name ~= "XercesHUD" then
                local name = gui.Name:lower()
                if name:match("loading") or name:match("intro") or name:match("welcome") then
                    pcall(function() gui:Destroy() end)
                end
            end
        end
    end
end)

task.spawn(function()
    while task.wait(0.03) do
        for _, button in pairs(player.PlayerGui:GetDescendants()) do
            if (button:IsA("TextButton") or button:IsA("ImageButton")) and button.Visible then
                pcall(function()
                    local text = ""
                    if button:IsA("TextButton") then
                        text = button.Text:lower()
                    elseif button.Name then
                        text = button.Name:lower()
                    end
                   
                    if text:match("play") or text:match("continue") or text:match("start") or text:match("skip") then
                        for _, connection in pairs(getconnections(button.MouseButton1Click)) do
                            connection:Fire()
                        end
                    end
                end)
            end
        end
    end
end)

task.spawn(function()
    while task.wait(5) do
        local success = pcall(function()
            if collectgarbage then
                collectgarbage("collect")
            end
        end)
    end
end)

task.spawn(function()
    task.wait(1)
    pcall(function()
        for _, obj in pairs(workspace:GetDescendants()) do
            if obj:IsA("Sound") and not obj:IsDescendantOf(player.Character) then
                obj:Stop()
                obj.Volume = 0
            end
        end
    end)
end)

task.spawn(function()
    task.wait(5)
    while task.wait(30) do
        local ping = 0
        local fps = 0
       
        pcall(function()
            ping = game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValue()
            fps = math.floor(1 / game:GetService("RunService").RenderStepped:Wait())
        end)
       
        instantLog(string.format("Performance: FPS=%d, PING=%.0f, BOOST=%s", fps, ping, fpsBoostEnabled and "ON" or "OFF"), "INFO")
    end
end)

instantLog("Starting XERCES SPEED initialization...", "INFO")

game:GetService("ReplicatedFirst"):RemoveDefaultLoadingScreen()

if not game:IsLoaded() then
    game.Loaded:Wait()
end

task.wait(0.5)

local loadingScreen = createPremiumLoadingScreen()
local hud = createPremiumHUD()

task.spawn(function()
    local playerGui = player:WaitForChild("PlayerGui")
    while task.wait(0.1) do
        if not hud.Parent or hud.Parent ~= playerGui then
            hud.Parent = playerGui
        end
        if loadingScreen and loadingScreen.Parent ~= playerGui then
            loadingScreen.Parent = playerGui
        end
    end
end)

if autoFPSBoost then
    task.wait(0.5)
    toggleFPSBoost()
    instantLog("Auto FPS Boost activated on startup", "INFO")
end

task.wait(0.2)
applyNightMode()
instantLog("XERCES SPEED fully loaded", "INFO")
instantLog("Press [L] to toggle FPS Boost", "INFO")
instantLog("Press [CTRL+L] to toggle Auto FPS Boost", "INFO")
instantLog("Instant log forwarding: ACTIVE", "INFO")
print("âš¡ XERCES SPEED - PREMIUM EDITION")
print("âš¡ Console logs are forwarded INSTANTLY")
